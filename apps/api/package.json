{
  "name": "@opale/api",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "exports": {
    "./registry": "./.adonisjs/client/registry.ts",
    "./data": "./.adonisjs/client/data.d.ts"
  },
  "scripts": {
    "start": "node bin/server.js",
    "build": "node ace build",
    "dev": "node ace serve --hmr",
    "test": "node ace test",
    "typecheck": "tsc --noEmit"
  },
  "imports": {
    "#app/*": "./app/*.js",
    "#core/*": "./app/core/*.js",
    "#identity/*": "./app/identity/*.js",
    "#controllers/*": "./app/controllers/*.js",
    "#exceptions/*": "./app/exceptions/*.js",
    "#models/*": "./app/models/*.js",
    "#mails/*": "./app/mails/*.js",
    "#services/*": "./app/services/*.js",
    "#listeners/*": "./app/listeners/*.js",
    "#events/*": "./app/events/*.js",
    "#generated/*": "./.adonisjs/server/*.js",
    "#middleware/*": "./app/middleware/*.js",
    "#transformers/*": "./app/transformers/*.js",
    "#validators/*": "./app/validators/*.js",
    "#providers/*": "./providers/*.js",
    "#policies/*": "./app/policies/*.js",
    "#abilities/*": "./app/abilities/*.js",
    "#database/*": "./database/*.js",
    "#tests/*": "./tests/*.js",
    "#start/*": "./start/*.js",
    "#config/*": "./config/*.js"
  },
  "dependencies": {
    "@adonisjs-community/girouette": "0.1.6-next.2",
    "@adonisjs/auth": "10.0.0-next.3",
    "@adonisjs/core": "7.0.0-next.17",
    "@adonisjs/cors": "3.0.0-next.0",
    "@adonisjs/drive": "^3.4.1",
    "@adonisjs/lucid": "22.0.0-next.1",
    "@adonisjs/session": "8.0.0-next.2",
    "@adonisjs/shield": "9.0.0-next.2",
    "@adonisjs/static": "2.0.0-next.1",
    "@monocle-app/agent": "1.0.0-beta.2",
    "@tuyau/core": "catalog:",
    "@vinejs/vine": "^4.2.0",
    "luxon": "^3.7.2",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "ts-pattern": "catalog:",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@adonisjs/assembler": "8.0.0-next.27",
    "@adonisjs/tsconfig": "2.0.0-next.3",
    "@japa/assert": "^4.2.0",
    "@japa/plugin-adonisjs": "5.1.0-next.0",
    "@japa/runner": "^4.4.0",
    "@poppinss/ts-exec": "^1.4.1",
    "@types/luxon": "^3.7.1",
    "hot-hook": "0.4.1-next.4",
    "pino-pretty": "^13.1.3",
    "youch": "4.1.0-beta.13"
  },
  "hotHook": {
    "boundaries": [
      "./app/**/controllers/**/*.ts",
      "./app/**/middleware/**/*.ts"
    ]
  }
}
